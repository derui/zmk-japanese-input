#include <dt-bindings/zmk/keys.h>
#include <behaviors.dtsi>
#include <behaviors/japanese_input.dtsi>
#include <dt-bindings/zmk/kscan_mock.h>

/ {
    keymap {
        compatible = "zmk,keymap";
        default_layer {
            bindings = <
                &kp A &kp B
                &kp C &num_word 1
            >;
        };

        num_layer {
            bindings = <
                &kp N0 &kp N1
                &trans &trans
            >;
        };
    };
};
